<DialogueTree xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/Outer-Wilds-New-Horizons/new-horizons/main/NewHorizons/Schemas/dialogue_schema.xsd">
	<DialogueNode>
		<Name>Tephra_RH_Hugged</Name>
		<EntryCondition>RH_TEPHRA_HUGGED</EntryCondition>
		<Dialogue>
			<Page>Thanks for the hug, astronaut!</Page>
		</Dialogue>
		<ConditionToCancel>RH_TEPHRA_HUGGED</ConditionToCancel>
	</DialogueNode>
	<DialogueNode>
		<Name>Tephra_RH_Boom</Name>
		<EntryCondition>RH_BOOM</EntryCondition>
		<Dialogue>
			<Page>…Huh? W-wait, what's happening? Astronaut, what's going on?</Page>
		</Dialogue>
		<SetCondition>TEPHRA_RH_SILENT</SetCondition>
	</DialogueNode>
	<DialogueNode>
		<Name>Tephra_RH_Silent</Name>
		<EntryCondition>TEPHRA_RH_SILENT</EntryCondition>
		<Dialogue>
			<Page>Astronaut, p-please, help me!</Page>
		</Dialogue>
	</DialogueNode>
	<DialogueNode>
		<Name>Tephra_RH_Silent_Galena</Name>
		<EntryCondition>TEPHRA_RH_SILENT</EntryCondition>
		<EntryCondition>RH_TEPHRA_WITH_GALENA</EntryCondition>
		<Dialogue>
			<Page>…I-I'll keep you safe, Galena!</Page>
		</Dialogue>
	</DialogueNode>
	<DialogueNode>
		<Name>Tephra_RH_Boom_Hugged</Name>
		<EntryCondition>RH_TEPHRA_HUGGED_SUPERNOVA</EntryCondition>
		<Dialogue>
			<Page><![CDATA[<i>Sniffle</i>]]>… th-thank you, astronaut…</Page>
		</Dialogue>
	</DialogueNode>
</DialogueTree>